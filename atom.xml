      <![CDATA[<!--

DO NOT ERASE THIS PAGE.

http://www.comtechreview.org/order.php

-->

<script language="JavaScript">

function computeSubtotal ( affiliation, quantity ) {

	var cost ;
	// FIXME: verify what the person entered is a number
	// need a couple loops to verify every character can be found in the set of [0-9]
	
	if ( quantity < 1 ) {

		alert ("Invalid quantity.") ;
		quantity = 1 ;
		document.order.quantity.value = quantity ;

	}

	// set the affiliation, if necessary

	if ( affiliation == 'empty' ) {
		for ( var i=0; i < document.order.affiliation.length; i++ ) {
			if ( document.order.affiliation[i].checked ) {
				affiliation = document.order.affiliation[i].value ;
			}
		}
	}

	if ( affiliation == 'other' ) {
		cost = 10 ;
	}
	else if ( affiliation == 'nten' )
	{
		cost = 7.5 ;
	}
	else
	{
		cost = 5 ;
	}


	// compute subtotal
	var subtotal = quantity * cost ;

	// pretty-print the subtotal
	if ( affiliation == 'nten' )
	{
		// if quantity is not divisble by two, add a 0
		if ( quantity%2 == 0 )
		{
			document.order.subtotal.value = subtotal ;
		}
		else
		{
			document.order.subtotal.value = subtotal+'0'; ;
		}
	}
	else
	{
		document.order.subtotal.value = subtotal ;
	}

	// update the PayPal subtotal (in hidden amount field)
	document.order.amount.value = cost ;

}

function checkIssue ( issue ) {

	if ( issue == 0 ) {
		alert("Please select an issue.") ;
		return false ;
	}
	else
	{
		document.order.item_name.value = issue ;
		return true ;
	}

}

</script>

